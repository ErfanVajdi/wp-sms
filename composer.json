{
    "name": "veronalabs/wp-sms-two-way",
    "type": "wordpress-plugin",
    "description": "Powerful integration version of WP-SMS for WooCommerce",
    "keywords": ["two-way", "incoming-message", "sms"],
    "homepage": "https://wp-sms-pro.com/two-way",
    "require": {
        "php"     : "^7.1",
        "veronalabs/updater": "^1.1",
        "monolog/monolog": "^2.3",
        "brick/phonenumber": "^0.4.0",
        "veronalabs/framework": "^1.3",
        "spatie/enum": "^3.12"
    },
    "autoload": {
        "psr-4": {
            "WPSmsTwoWayPlugin\\": "",
            "WPSmsTwoWay\\": "src/"
        }
    },
    "config": {
        "platform-check": false,
        "preferred-install": {
            "veronalabs/framework": "source"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "require-dev": {
        "humbug/php-scoper": "*@dev",
        "symfony/console": "^5.4"
    }
}
